.noshop-card {
  width: 100%;
  margin-top: 40px;
  padding: 0px 16px;
}
.noshop-image {
  width: 100%;
  height: 806px;
}

.stroll-fotter {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 20px;
}
.fot-twoimg {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.dislike-image {
  width: 120px;
  height: 120px;
}
.like-image {
  width: 120px;
  height: 120px;
  margin-left: 48px;
}

.fot-text {
  color: #368BE5;
  font-size: 28px;
  margin-top: 40px;
  margin-bottom: 50px;
}
