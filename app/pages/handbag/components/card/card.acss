.stroll-shop {
  width: 100%;
  height: 350px;
  margin-top: 40px;
  margin-left: 5px;
}
.shop-imgbox {
  display: flex;
  align-items: center;
  width: 700rpx;
  height: 850rpx;
  border-radius: 24rpx;
}
.images-box {
  width: 660rpx;
  height: 900rpx;
  border-radius: 24rpx;
  box-shadow: 0rpx 2rpx 5rpx 0rpx rgba(0,0,0,0.1);
  background-color: #fff;
  position: relative;
}
.images {
  width: 300rpx;
  height: 300rpx;
  position: relative;
  left: 20rpx;
  top: 20rpx;
  display: flex;
  float: left;
}
.basic-info {
  position: relative;
  display: flex;
  left: 50rpx;
  top: 30rpx;
  /* font-size: 40rpx; */
  /* float: left; */
}
.title {
  /* position: relative; */
  /* display: flex; */
  /* left: 50rpx; */
  /* top: 30rpx; */
  font-size: 40rpx;
  line-height: 150%;
  /* float: left; */
}
.shop {
  font-size: 25rpx;
 
}
.divider {
  position:relative;
  top: 185rpx;
  border-top: 1rpx solid #b8b5b5;
  margin: 20rpx 24rpx;
}
.tag {
  position: relative;
  top: 200rpx;
  left: -250rpx;
  padding: 10rpx 4rpx 10rpx;
}
.order-section-body__tag {
  background: #ddb6b987;
  font-size: 26rpx;
  color: rgb(139, 139, 139);
  line-height: 30rpx;
  padding: 10rpx 12rpx 10rpx 12rpx;
  border-radius: 6rpx;
  margin-bottom: 15rpx;
  margin-right: 20rpx;
  display: inline-block;
}
.description {
  position: relative;
  top: 200rpx;
  right: 150rpx;
  text-align: center;
  /* left: -250rpx; */
  font-size: 36rpx;
}

.img_notwin {
  position: relative;
  z-index: -1;
  left: 20px;
  width: 320px;
  height: 320px;
}

.img_win {
  position: relative;
  z-index: -1;
  left: 9px;
  width: 360px;
  height: 94px;
}








.imagelike {
  width: 96px;
  height: 48px;
  position: absolute;
  right: 40px;
  top: 20px;
}
.imageunlike {
  width: 148px;
  height: 48px;
  position: absolute;
  left: 40px;
  top: 20px;
}

