.stroll-shop {
  width: 100%;
  height: 350px;
  margin-top: 40px;
  margin-left: 10px;
}
.shop-imgbox {
  display: flex;
  align-items: center;
  width: 700rpx;
  height: 600rpx;
  border-radius: 24rpx;
}
.images-box {
  width: 660rpx;
  height: 100%;
  border-radius: 24rpx;
  box-shadow: 0rpx 2rpx 5rpx 0rpx rgba(0,0,0,0.1);
  background-color: #fff;
  position: relative;
}
.images {
  width: 480rpx;
  height: 480rpx;
  position: absolute;
  left: 40rpx;
  top: 20rpx;
}
.imagelike {
  width: 96px;
  height: 48px;
  position: absolute;
  right: 40px;
  top: 20px;
}
.imageunlike {
  width: 148px;
  height: 48px;
  position: absolute;
  left: 40px;
  top: 20px;
}

