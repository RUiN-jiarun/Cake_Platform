.stroll-tab {
  width: 100%;
  min-height: 100vh;
  background: #F6F6F6;
}
.stroll-text {
  width: 100%;
  margin-top: 40px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.text-tip1 {
  font-size: 28px;
  color: #333333;
}
.text-tip2 {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.t1 {
  font-size: 28px;
  color: #333333;
}
.icon-image {
  width:20px;
  height:20px;
}


